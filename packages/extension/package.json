{
  "name": "vscode-extension",
  "displayName": "vscode-async-webview-sample",
  "description": "this is just a sample",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "packages/extension/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "myExtension.start",
        "title": "Start new webview"
